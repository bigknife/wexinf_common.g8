<web-app>
    <servlet>
        <servlet-name>$name;format="Camel"$ServerServlet</servlet-name>
        <servlet-class>$name;format="snake"$.server.world.servlet.$name;format="Camel"$ServerServlet</servlet-class>
        <async-supported>true</async-supported>
    </servlet>

    <servlet-mapping>
        <servlet-name>$name;format="Camel"$ServerServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>
